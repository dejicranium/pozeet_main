<div id="not_home_navbar" style="display: flex;
    padding: 10px 10px 10px 10px;
    width: 100%;
    min-height: 35px;
    background-color: white;
    position: fixed;
    top: 0;
    color: darkgrey;
    flex-direction: row;
    box-shadow: 0px 0px 2px 0px grey;
    margin-bottom: 10px;
    z-index: 5;
    box-sizing: border-box;">
    {% if request.referer %}
    <i class="fas fa-arrow-left" style="align-self:center; justify-self: flex-start;" onclick="window.open('{{request.referer}}', '_self')"></i>
    {% endif %}
    <p class="home-text" onclick="window.open('/mobile/feed', '_self')" style="align-self: center; justify-self: center; font-weight:bold; margin:auto; text-align: center; width: 50%;">Home</p>  
</div>
